{{#unless reactJS.answers.createInterface }}
type Props = {
{{#if reactJS.answers.propList }}
{{#each reactJS.answers.propList}}
  {{ this }}?: string;
{{/each}}
{{else}}
  text?: string;
{{/if}}
};
{{/unless}}

{{#unless reactJS.answers.createInterface }}
const defaultProps: Props = {
{{#if reactJS.answers.propList }}
